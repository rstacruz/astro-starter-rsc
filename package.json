{
  "name": "astro-starter-rsc",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "astro:check": "astro check",
    "eslint:check": "eslint . --cache --cache-location .cache/eslint",
    "eslint:format": "eslint . --cache --cache-location .cache/eslint --fix",
    "prettier:check": "prettier --cache --cache-location .cache/prettier --check .",
    "prettier:format": "prettier --cache --cache-location .cache/prettier --write . --log-level silent",
    "check": "astro sync && FORCE_COLOR=3 pnpm run '/^(astro:check|eslint:check|prettier:check|vitest:run)$/'",
    "format": "pnpm run --sequential '/^(eslint:format|prettier:format)$/'"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.2.1",
    "@eslint/js": "^9.22.0",
    "@tailwindcss/vite": "^4.0.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "^3.0.8",
    "astro": "^5.5.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.14",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  },
  "packageManager": "pnpm@10.8.0+sha512.0e82714d1b5b43c74610193cb20734897c1d00de89d0e18420aebc5977fa13d780a9cb05734624e81ebd81cc876cd464794850641c48b9544326b5622ca29971",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
