{
  "name": "astro-starter-rsc",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "test": "vitest",
    "astro:check": "astro check",
    "eslint:check": "eslint . --cache --cache-location .cache/eslint",
    "eslint:format": "eslint . --cache --cache-location .cache/eslint --fix",
    "prettier:check": "prettier --cache --cache-location .cache/prettier --check .",
    "prettier:format": "prettier --cache --cache-location .cache/prettier --write . --log-level silent",
    "typescript:check": "tsc --noEmit",
    "check": "concurrently --pad-prefix -c green,blue,yellow,red --names eslint,prettier,astro,typescript --group \"pnpm --silent eslint:check\" \"pnpm --silent prettier:check\" \"pnpm --silent astro:check\" \"pnpm --silent typescript:check\"",
    "format": "concurrently -m 1 --pad-prefix -c green,blue,yellow,red --names eslint,prettier --group \"pnpm --silent eslint:format\" \"pnpm --silent prettier:format\""
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/react": "^4.2.1",
    "@eslint/js": "^9.22.0",
    "@tailwindcss/vite": "^4.0.14",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitest/coverage-v8": "^3.0.8",
    "astro": "^5.5.2",
    "concurrently": "^9.1.2",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "jsdom": "^26.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-organize-imports": "^4.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.0.14",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.26.1",
    "vitest": "^3.0.8"
  },
  "packageManager": "pnpm@10.4.0+sha512.6b849d0787d97f8f4e1f03a9b8ff8f038e79e153d6f11ae539ae7c435ff9e796df6a862c991502695c7f9e8fac8aeafc1ac5a8dab47e36148d183832d886dd52",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  }
}
